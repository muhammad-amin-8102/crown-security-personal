{
  "name": "crown-security-backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm install && echo 'Build completed'",
    "build:web": "cd ../app/crown_security && flutter build web --release --dart-define=API_BASE_URL=/api/v1 && cp -r build/web/* ../../../backend/public/admin/",
    "build:web:win": "cd ..\\app\\crown_security && flutter build web --release --dart-define=API_BASE_URL=/api/v1 && xcopy build\\web\\* ..\\..\\..\\backend\\public\\admin\\ /E /Y",
    "deploy": "npm run db:create && npm run db:migrate && npm run db:seed",
    "render-build": "npm install",
    "render-start": "node src/server.js",
    "docker:build": "docker build -t crown-security-api .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.docker crown-security-api",
    "docker:dev": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker-compose down",
    "test-db": "node test-db-connection.js",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.5",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
